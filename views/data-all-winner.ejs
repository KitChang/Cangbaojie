
    
      <div class="container content">
        <div class="row">
            <div class="panel panel-default">
                <div class="panel-heading"><a href="http://<%=req.host%>:<%=req.port%>/advertisement">奖项</a></div>   
                <form>
                
                <table class="table table-striped">
                    <thead>
                    <tr><th>奖项</th><th>用户</th><th>性别</th><th>联系电话</th></tr>
                    </thead>
                    
                    <tbody>
                <%
                var row;
                while (resultArr.length) {
                    var username = "";
                    var gender = "";
                    var phone = "";
                    var prizeMap = {};
                    prizeMap['1'] = "一等奖";
                    prizeMap['2'] = "二等奖";
                    prizeMap['3'] = "三等奖";
                    prizeMap['4'] = "四等奖";
                    prizeMap['5'] = "五等奖";
                    row = resultArr.pop();
                    if(row.app_user!=null){
                        //console.log(row.app_user);
                        username = row.app_user.username;
                        gender = row.app_user.gender;
                        phone = row.app_user.phone;
                    }
                    if(row.prize){
                        prize = prizeMap[row.prize];
                        if(prize==null)
                            prize = "";
                    }
                    
                %>
                    <tr><td><%=prize%></td><td><%=username%></td><td><%=gender%></td><td><%=phone%></td>
                    
                    </tr>
                <%
                }
                %>
                    </tbody>
                </table>
                </form>

            </div>
        </div>
        
      </div>
    <script type="text/javascript" src="/js/dependencies/jquery.min.js"></script>
    <script type="text/javascript" src="/bootstrap/js/bootstrap.min.js"></script>
    


